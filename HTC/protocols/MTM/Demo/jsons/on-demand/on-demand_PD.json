                        {
                            "Name": "Crisis Support",
                            "Title": "Crisis Support",
                            "Type": "Survey",
                            "Pages": [
                                {
                                    "Conditions": [
                                        {
                                            "VariableName": "subdomain_chosen",
                                            "Value": "Crisis_Support"
                                        }
                                    ],
                                    "Inputs": [
                                        {
                                            "Type": "Text",
                                            "Parameters": {
                                                "IsHtml": true,
                                                "Text": "<b><font color=\"#9769ED\" size=6>988 Suicide & Crisis Lifeline</font></b><br /><br />The 988 Suicide & Crisis Lifeline offers free and confidential support 24/7 for mental health-related distress. If you or someone you know is struggling or in crisis, call or text 988 or chat 988lifeline.org/chat to connect with a trained crisis counselor.<br /><br /><a href=\"https://988lifeline.org\">https://988lifeline.org</a><br /><br /><br /><br />"
                                            }
                                        }
                                    ],
                                    "CanBeFavorited": true
                                }
